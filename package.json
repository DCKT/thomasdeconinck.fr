{
  "name": "blog",
  "version": "1.11.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 node_modules/canvas/build/Release/ && yarn build",
    "start": "next start",
    "lint": "next lint",
    "intl:extract": "formatjs extract 'pages/**/*.js*' 'components/**/*.ts*' --out-file locales/en.json",
    "intl:compile": "formatjs compile locales/en.json --ast --out-file compiled-lang/en.json",
    "intl": "npm run extract:intl && npm run compile:intl",
    "generate-sitemap": "node scripts/generate-sitemap.js"
  },
  "dependencies": {
    "autoprefixer": "10.4.19",
    "canvas": "2.11.2",
    "clsx": "2.1.1",
    "cors": "2.8.5",
    "date-fns": "3.6.0",
    "datocms-client": "3.5.24",
    "globby": "14.0.1",
    "graphql": "15.7.1",
    "graphql-request": "3.7.0",
    "lodash": "4.17.21",
    "next": "14.2.3",
    "postcss": "8.4.38",
    "prismjs": "1.29.0",
    "react": "18.3.1",
    "react-datocms": "3.0.8",
    "react-dom": "18.3.1",
    "react-icons": "5.1.0",
    "react-intl": "6.6.5",
    "react-use": "17.5.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-external-links": "1.0.1",
    "rehype-highlight": "5.0.2",
    "rehype-rewrite": "3.0.4",
    "rehype-slug": "5.0.1",
    "rehype-stringify": "9.0.3",
    "remark-parse": "10.0.2",
    "remark-prism": "1.3.6",
    "remark-rehype": "10.1.0",
    "tailwindcss": "3.4.3",
    "unified": "10.1.2",
    "unified-stream": "2.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "7.24.4",
    "@formatjs/cli": "4.8.2",
    "babel-plugin-formatjs": "10.5.14"
  }
}
