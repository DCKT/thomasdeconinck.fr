{
  "name": "blog",
  "version": "1.8.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 node_modules/canvas/build/Release/ && yarn build",
    "start": "next start",
    "lint": "next lint",
    "intl:extract": "formatjs extract 'pages/**/*.js*' 'components/**/*.ts*' --out-file locales/en.json",
    "intl:compile": "formatjs compile locales/en.json --ast --out-file compiled-lang/en.json",
    "intl": "npm run extract:intl && npm run compile:intl",
    "generate-sitemap": "node scripts/generate-sitemap.js"
  },
  "dependencies": {
    "autoprefixer": "10.4.2",
    "canvas": "2.6.1",
    "clsx": "1.1.1",
    "cors": "2.8.5",
    "date-fns": "2.28.0",
    "datocms-client": "3.5.14",
    "globby": "13.1.1",
    "graphql": "15.7.1",
    "graphql-request": "3.7.0",
    "lodash": "4.17.21",
    "next": "12.0.9",
    "postcss": "8.4.5",
    "prismjs": "1.26.0",
    "react": "17.0.2",
    "react-datocms": "3.0.0",
    "react-dom": "17.0.2",
    "react-icons": "4.3.1",
    "react-intl": "5.24.4",
    "react-use": "17.3.2",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-external-links": "1.0.1",
    "rehype-highlight": "5.0.2",
    "rehype-rewrite": "3.0.4",
    "rehype-slug": "5.0.1",
    "rehype-stringify": "9.0.2",
    "remark-parse": "10.0.1",
    "remark-prism": "1.3.6",
    "remark-rehype": "10.1.0",
    "tailwindcss": "3.0.18",
    "unified": "10.1.1",
    "unified-stream": "2.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "7.16.7",
    "@formatjs/cli": "4.8.1",
    "babel-plugin-formatjs": "10.3.17"
  }
}
